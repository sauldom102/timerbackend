{
    "dependencies": {
        "@prisma/client": "^3.7.0",
        "@types/cors": "^2.8.12",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.13",
        "@types/helmet": "^4.0.0",
        "@types/jest": "^27.0.3",
        "@types/jsonwebtoken": "^9.0.0",
        "@types/lodash": "^4.14.186",
        "@types/node": "^17.0.4",
        "@types/nodemailer": "^6.4.7",
        "@types/supertest": "^2.0.11",
        "@types/swagger-ui-express": "^4.1.3",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "date-fns": "^2.29.3",
        "dotenv": "^10.0.0",
        "express": "^4.17.2",
        "express-http-context": "^1.2.4",
        "helmet": "^4.6.0",
        "jest": "^27.4.5",
        "jsonwebtoken": "^9.0.0",
        "lodash": "^4.17.21",
        "node-fetch": "^3.2.10",
        "nodemailer": "^6.9.0",
        "patch-package": "^6.5.1",
        "prisma": "^3.7.0",
        "stripe": "^11.6.0",
        "supertest": "^6.1.6",
        "swagger-ui-express": "^4.6.0",
        "ts-jest": "^27.1.2",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^1.1.8",
        "tsconfig-paths": "^4.1.0",
        "typescript": "^4.5.4",
        "typescript-transform-paths": "^3.4.6",
        "zod": "^3.20.2",
        "zod-express-middleware": "^1.4.0",
        "@types/bcryptjs": "^2.4.2"
    },
    "jest": {
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "transform": {
            "\\.(ts|tsx)$": "ts-jest"
        }
    },
    "name": "timerbackend",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "dev": "ts-node-dev -r tsconfig-paths/register --respawn --pretty --transpile-only src/index.ts",
        "build": "tsc --build tsconfig.json && yarn copy-files",
        "copy-files": "cp -R ./src/public/ ./dist/src/public/",
        "start": "node -r ts-node/register --max-old-space-size=2560 dist/src/index.js",
        "test": "jest",
        "generate-prisma": "prisma generate",
        "postinstall": "patch-package && tsc --build tsconfig.json",
        "set-dev-env": "rm .env && cp .env.dev .env",
        "set-prod-env": "rm .env && cp .env.prod .env",
        "deploy": "yarn set-prod-env && git push heroku main",
        "seed": "ts-node --transpile-only prisma/seed.ts"
    },
    "author": "",
    "license": "ISC",
    "description": ""
}
